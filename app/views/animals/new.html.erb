<h1>Hi <%#= current_user.name.capitalize %>! Let's get started listing your pet</h1>
<%= simple_form_for(animals_path) do |f| %>
<%= f.input :name %>
<%= f.input :species, collection: Animal::SPECIES %>
<%= f.input :characteristics, as: :text, input_html: {rows: 4} %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>


<% end %>

